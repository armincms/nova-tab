{
    "name": "armincms/nova-tab",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova",
        "tab",
        "armincms"
    ],
    "authors": [
        {
            "name": "Ismail Zareh",
            "email": "zarehesmaiel@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Tab\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Tab\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
